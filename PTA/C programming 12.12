#include<stdio.h>
int main(){
	double x1, y1, x2, y2,add,addi,mul,muli;
	scanf("%lf %lf %lf %lf", &x1, &y1, &x2, &y2);
	add = x1 + x2;
	addi = y1 + y2;
	mul = x1 * x2 - y1 * y2;
	muli = x1 * y2 + x2 * y1;
	printf("addition of complex is %lf+%lfi\nproduct of complex is %lf+%lfi", add, addi, mul, muli);
}

