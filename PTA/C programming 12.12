#include <stdio.h>
#include <stdlib.h>
int a[11],b[11];
int n;
void find(int a[],int b[],int x)
{
    if(x==n+1)
    {
        for(int i=1; i<=n; i++)
            printf("%d",b[i]);
        printf("\n");
    }
    for(int i=1; i<=n; i++)
    {
        if(a[i]==0)
        {
            a[i]=1;
            b[x]=i;
            find(a,b,x+1);
            a[i]=0;
        }
    }
}
int main()
{
    scanf("%d",&n);
    find(a,b,1);
    return 0;
}

