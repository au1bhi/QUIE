#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cin>>a>>b>>c;
    cout<<"Name   Counter"<<endl;
    cout<<"--------------"<<endl;
    printf("John%10d\n",a);
    printf("Sally%9d\n",b);
    printf("Tom%11d\n",c);
    cout<<"--------------"<<endl;
    printf("Total%9d\n",(a+b+c));
}
