#include<iostream>
using namespace std;
int main() {
    float a; int b; char c; float a2;
    cin >> a >> b >> c >> a2;
    scanf("%f %d %c %f", &a, &b, &c, &a2);
    printf("%c %d %.2lf %.2lf", c, b, a, a2);
}

