#include <stdio.h>
int cf(int a) {
    int t = 1, res = 1;
    while (t <= a) {
        res *= t;
        t++;
    }
    return res;
}
int main() {
    int n, resu = 0;
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        resu += cf(i);
    }
    printf("%d\n", resu);
}

--------------------------------------
#include <stdio.h>

int main() {
    char c;
    int count = 0;
    int in_word = 0;

    while ((c = getchar()) != '\n') {
        if (c == ' ') {
            in_word = 0;
        } else if (in_word == 0) {
            in_word = 1;
            count++;
        }
    }

    printf("%d\n", count);

    return 0;
}

--------------------------------------
#include <stdio.h>
int main() {
    int a1, a2, n, count = 3, num[1005], i, j, sum;
    scanf("%d %d %d", &a1, &a2, &n);
    num[1] = a1, num[2] = a2;
    for (i = 1; i <= n; i++) {
        sum = num[i] * num[i + 1];
        if (sum < 10) {
            num[count++] = sum;
        } else {
            num[count++] = sum / 10;
            num[count++] = sum % 10;
        }
        if (count > n) break;
    }
    printf("%d", a1);
    for (j = 2; j <= n; j++) {
        printf(" %d", num[j]);
    }
    return 0;
}

--------------------------------------
#include <stdio.h>
int main() {
    int n, max, index, i;
    scanf("%d", &n);
    int a[n];
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    max = a[0];
    index = 0;
    for (i = 1; i < n; i++) {
        if (a[i] > max) {
            max = a[i];
            index = i;
        }
    }
    printf("%d %d", max, index);

    return 0;
}
