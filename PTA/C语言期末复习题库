void delchar(char* str, char c) {
    int i, j;
    for (i = 0, j = 0; str[i] != '\0'; i++) {
        if (str[i] != c) {
            str[j++] = str[i];
        }
    }
    str[j] = '\0';
}

--------------------------------------
void sum_diff( float op1, float op2, float *psum, float *pdiff ){
    *psum=op1+op2;
    *pdiff=op1-op2;
}

--------------------------------------
void StringCount(char *s) {
    int result[] = {0, 0, 0, 0, 0};
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] >= 'A' && s[i] <= 'Z') {
            result[0] += 1;
        } else if (s[i] >= 'a' && s[i] <= 'z') {
            result[1] += 1;
        } else if (s[i] == ' ') {
            result[2] += 1;
        } else if (s[i] >= '0' && s[i] <= '9') {
            result[3] += 1;
        } else {
            result[4] += 1;
        }
    }
    for (int j = 0; j <= 3; j++) {
        printf("%d ", result[j]);
    }
    printf("%d",result[4]);
}

--------------------------------------
int Ack( int m, int n ){
    if (m==0)return n+1;
    if (n==0 && m>0) Ack(m-1,1);
    if (m>0 && n>0) Ack(m-1,Ack(m,n-1));
}

--------------------------------------
#include <stdio.h>
int cf(int a) {
    int t = 1, res = 1;
    while (t <= a) {
        res *= t;
        t++;
    }
    return res;
}
int main() {
    int n, resu = 0;
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        resu += cf(i);
    }
    printf("%d\n", resu);
}

--------------------------------------
#include <stdio.h>

int main() {
    char c;
    int count = 0;
    int in_word = 0;

    while ((c = getchar()) != '\n') {
        if (c == ' ') {
            in_word = 0;
        } else if (in_word == 0) {
            in_word = 1;
            count++;
        }
    }

    printf("%d\n", count);

    return 0;
}

