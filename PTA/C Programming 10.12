    double H,W;
    scanf("%lf %lf", &H, &W);
    printf("身高%d英尺%.2lf英寸,体重%.2lf磅", (int)(H / 2.54 / 12), (H / 2.54) - (int)(H / 2.54 / 12)*12, W / 0.45359237);
}

